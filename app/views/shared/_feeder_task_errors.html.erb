<% if @feeder_task.errors.any? %>
	<div id="error_explanation">
		<div class="alert alert-error">
			The form contains <%= pluralize(@task_set.errors.count, "error") %>.
		</div>
		<ul>
		<% @feeder_task.errors.full_messages.each do |msg| %>
			<li>* <%= msg %></li>
		<% end %>
		</ul>
	</div>
<% end %>